@{ 
    var employees = ViewData["employees"] as IEnumerable<WBDDemo.Models.Employee>;

}
@foreach (var employee in employees)
{
    <div>

        <h3>@employee.Fullname</h3>
        <img src="@Url.Content(employee.AvatarPath)"alt="Alternate Text" />
        <h4>@employee.Email</h4>
        <h4>@employee.Department</h4>
    </div>

}